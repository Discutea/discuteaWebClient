{
  "name": "discuteaWebClient",
  "description": "The self-hosted Web IRC client",
  "version": "2.2.1",
  "preferGlobal": true,
  "bin": {
    "lounge": "index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Discutea/discuteaWebClient.git"
  },
  "scripts": {
    "start": "node index",
    "start-dev": "npm-run-all --parallel watch start",
    "build": "npm-run-all build:*",
    "build:font-awesome": "node scripts/build-fontawesome.js",
    "build:webpack": "webpack",
    "watch": "webpack --watch",
    "prepublishOnly": "NODE_ENV=production npm run build"
  },
  "keywords": [
    "lounge",
    "browser",
    "web",
    "chat",
    "client",
    "irc",
    "server"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=4.2.0"
  },
  "dependencies": {
    "accept-language": "3.0.15",
    "bcrypt-nodejs": "0.0.3",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "commander": "2.9.0",
    "daemonize2": "~0.4.2",
    "emoji-name-map": "^1.2.2",
    "event-stream": "3.3.4",
    "express": "4.15.2",
    "fs-extra": "2.0.0",
    "geoip-lite": "^1.2.1",
    "irc-framework": "2.10.3",
    "json-loader": "^0.5.4",
    "lodash": "4.17.4",
    "moment": "2.17.1",
    "noty": "2.4.1",
    "read": "1.0.7",
    "request": "2.79.0",
    "semver": "5.3.0",
    "socket.io": "1.7.3",
    "spdy": "3.4.4",
    "fips": "0.1.2"
  },
  "devDependencies": {
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-preset-es2015": "6.22.0",
    "chai": "3.5.0",
    "font-awesome": "4.7.0",
    "handlebars": "4.0.6",
    "handlebars-loader": "1.4.0",
    "npm-run-all": "4.0.2",
    "socket.io-client": "1.7.3",
    "webpack": "2.2.1"
  }
}
